[user]
  name = Erik Hollembeak
  email = erik.hollembeak@gmail.com
[alias]
  b = branch
  unstage = reset HEAD
  l = log --graph --pretty=colored --abbrev-commit --date=relative
  workprofile = 'config user.email "erik@bluebox.net"'
[pretty]
  colored = format: %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
